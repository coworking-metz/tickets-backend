	"item": [
		{
			"name": "Se connecter & obtenir un token",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test('Valeur de session_id', function() {\r",
							"\r",
							"// Store the session_id in a variable\r",
							"var token = pm.response.json().token;\r",
							"\r",
							"// Set the session_id as a variable to be used in other requests\r",
							"pm.globals.set(\"token\", token);\r",
							"\r",
							"})\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Authorization",
						"value": "Token {{app_token}}",
						"type": "text"
					},
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text",
						"disabled": true
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n\"identifiant\":\"{{identifiant}}\",\r\n\"password\":\"{{password}}\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{api_url}}/api/connexion",
					"host": [
						"{{api_url}}"
					],
					"path": [
						"api",
						"connexion"
					],
					"query": [
						{
							"key": "",
